<h1>New Visit</h1>

<%= form_with model: @visit, url: visits_path, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <%= form.hidden_field :user_id, value: User.first.id %>

  <div>
    <%= form.submit 'Create Place' %>
  </div>
<% end %>

<%= link_to 'Back', root_path %>
